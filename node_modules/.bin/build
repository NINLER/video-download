#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/Grey/Project/Client/video-download/node_modules/.store/nwjs-builder-phoenix@1.15.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/Grey/Project/Client/video-download/node_modules/.store/nwjs-builder-phoenix@1.15.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/nwjs-builder-phoenix@1.15.0/node_modules/nwjs-builder-phoenix/dist/bin/build.js" "$@"
else
  exec node  "$basedir/../.store/nwjs-builder-phoenix@1.15.0/node_modules/nwjs-builder-phoenix/dist/bin/build.js" "$@"
fi
